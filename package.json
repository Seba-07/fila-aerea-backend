{
  "name": "fila-aerea-backend",
  "version": "1.0.0",
  "description": "Backend API para gestión de filas y embarques en festival aéreo",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "ts-node src/scripts/seed.ts",
    "clear-db": "ts-node src/scripts/clearDatabase.ts",
    "init-staff": "ts-node src/scripts/initStaff.ts",
    "test": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "festival",
    "flights",
    "queue",
    "boarding"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/multer": "^2.0.0",
    "@types/pdfkit": "^0.17.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.17.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "socket.io": "^4.6.0",
    "swagger-ui-express": "^5.0.0",
    "transbank-sdk": "^6.1.0",
    "web-push": "^3.6.7",
    "yamljs": "^0.3.0"
